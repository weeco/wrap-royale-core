{
  "name": "wrap-royale-core",
  "version": "1.1.0",
  "description": "A promise based, minimalist wrapper for the official Clash Royale API written in TypeScript.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "lint": "tslint --project tsconfig.json --format stylish src/**/*.ts",
    "format": "prettier --write \"**/*.ts\"",
    "test": "mocha -r ts-node/register src/**/*.spec.ts",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weeco/wrap-royale-core.git"
  },
  "keywords": [
    "clash",
    "royale",
    "api",
    "wrapper",
    "typescript"
  ],
  "author": "Weeco <weeco91@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weeco/wrap-royale-core/issues"
  },
  "homepage": "https://github.com/weeco/wrap-royale-core#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "chai": "^4.1.2",
    "dotenv": "^5.0.1",
    "joi": "^13.1.2",
    "mocha": "^5.0.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/dotenv": "^4.0.2",
    "@types/joi": "^13.0.5",
    "@types/mocha": "^2.2.48",
    "rimraf": "^2.6.2",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.7.2"
  }
}
